<!DOCTYPE html>

<html>
    <head>
        <title>Open Follow-ups</title>
        
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <script src="~/Scripts/jquery-1.11.1.min.js"></script>
        
        <link href="/Scripts/bootstrap-3.2.0-dist/css/bootstrap.min.css" rel="stylesheet"/>
        <link href="~/Scripts/bootstrap-3.2.0-dist/css/datepicker.css" rel="stylesheet" />
        
        <script src="/Scripts/bootstrap-3.2.0-dist/js/bootstrap.min.js"></script>
        <script src="~/Scripts/bootstrap-3.2.0-dist/js/bootstrap-datepicker.js"></script>   
        <link href="~/styles.css" rel="stylesheet" />
        <script language="javascript">

            $(document).ready(function() {

                getOpenFollowUps();
            });

            function getOpenFollowUps() {
                $.ajax({
                        url: '/FollowUp/GetOpenFollowUps',
                        contentType: 'application/html; charset=utf-8',
                        type: 'GET',
                        dataType: 'html'
                    })
                    .success(function(result) {

                        $("#openFollowUps").html(result);


                    }).error(function(xhr, status) {
                        alert('Error');

                    });
            }

        </script>
    </head>
    <body>
        <div id="openFollowUps" style="position: absolute; top:10%; left:10%; width: 80%;"></div>
        <div class="modal fade" id="patientDetailModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>
</body>
</html>